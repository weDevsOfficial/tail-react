{
  "name": "@wedevs/tail-react",
  "version": "0.5.1",
  "description": "A React UI component library based on Tailwind CSS",
  "author": "Tareq Hasan",
  "license": "MIT",
  "homepage": "https://github.com/weDevsOfficial/tail-react",
  "repository": {
    "type": "git",
    "url": "https://github.com/weDevsOfficial/tail-react.git"
  },
  "bugs": {
    "url": "https://github.com/weDevsOfficial/tail-react/issues"
  },
  "keywords": [
    "react",
    "tailwind",
    "tailwindcss",
    "tailwindcss-components",
    "tailwindcss-react"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.16",
    "@headlessui/tailwindcss": "^0.2.0",
    "@heroicons/react": "^2.0.18",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-tooltip": "^1.0.7",
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.14",
    "@storybook/addon-interactions": "^7.6.14",
    "@storybook/addon-links": "^7.6.14",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.6.14",
    "@storybook/manager-api": "^7.6.14",
    "@storybook/react": "^7.6.14",
    "@storybook/react-vite": "^7.6.14",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/theming": "^7.6.14",
    "@tailwindcss/forms": "^0.5.4",
    "@types/node": "^20.4.7",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "^0.6.13",
    "path": "^0.12.7",
    "postcss": "^8.4.27",
    "storybook": "^7.6.14",
    "storybook-dark-mode": "^3.0.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.4.0"
  },
  "packageManager": "yarn@4.1.0"
}
